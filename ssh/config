# Defaults
host *
    Ciphers aes128-gcm@openssh.com,aes256-gcm@openssh.com,chacha20-poly1305@openssh.com,aes256-ctr,aes192-ctr,aes128-ctr
    Compression yes
    ControlMaster auto
    ControlPath /tmp/%u:%r@%h:%p
    ControlPersist 120
    ForwardAgent yes
    ServerAliveCountMax 6
    ServerAliveInterval 15
    TCPKeepAlive yes
